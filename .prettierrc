{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "crlf",
	"printWidth": 100,
	"importOrder": ["^[a-z]$", "^@", "^[./]"],
	"importOrderSeparation": true
}
