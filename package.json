{
	"name": "my-app-backend",
	"version": "0.0.1",
	"description": "This is the backend of my-app",
	"main": "build/index.js",
	"module": "src/index.js",
	"requiredVariables": [
		"SAMPLE"
	],
	"scripts": {
		"build": "babel src --out-dir build",
		"debug": "nodemon --inspect --exec babel-node src/index.js",
		"dev": "nodemon --exec babel-node src/index.js --ignore jsconfig.json",
		"start": "node build/index.js",
		"format": "prettier --write \"**/*.{js,mjs,cjs}\"",
		"format:check": "prettier -c \"**/*.{js,mjs,cjs}\"",
		"lint": "eslint \"src/**/*.{js,mjs,cjs}\"",
		"lint:fix": "npm run lint -- --fix",
		"prepare": "husky install",
		"test": "jest"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"chalk": "^4.1.1",
		"config": "^3.3.6",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"express-rate-limit": "^5.2.6",
		"helmet": "^4.6.0",
		"http-status-codes": "^2.1.4",
		"morgan": "^1.10.0",
		"swagger-ui-express": "^4.1.6",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/node": "^7.14.5",
		"@babel/plugin-proposal-throw-expressions": "^7.14.5",
		"@babel/plugin-transform-runtime": "^7.14.5",
		"@babel/preset-env": "^7.14.5",
		"@babel/runtime": "^7.14.6",
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@trivago/prettier-plugin-sort-imports": "^2.0.2",
		"@types/jest": "^26.0.23",
		"babel-plugin-module-resolver": "^4.1.0",
		"eslint": "^7.28.0",
		"eslint-plugin-jest": "^24.3.6",
		"husky": "^7.0.1",
		"jest": "^27.0.4",
		"nodemon": "^2.0.7",
		"prettier": "^2.3.1",
		"pretty-quick": "^3.1.1",
		"supertest": "^6.1.4"
	}
}
